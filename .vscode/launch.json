    "version": "0.2.0",
    "configurations": [

        {
            "name": "dataset list",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": ["--key-file=${workspaceFolder}/key.txt","dataset","list"]
        },
        {
          "name": "ds ls -r",
          "type": "go",
          "request": "launch",
          "mode": "debug",
          "program": "${workspaceFolder}",
          "args": ["--key-file=${workspaceFolder}/key.txt","dataset","list","--recursive"]
        },
        {
            "name": "dataset list dozer",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": ["--key-file=${workspaceFolder}/key.txt","dataset","list","dozer"]
        },
        {
          "name": "dataset ls -r dozer",
          "type": "go",
          "request": "launch",
          "mode": "debug",
          "program": "${workspaceFolder}",
          "args": ["--key-file=${workspaceFolder}/key.txt","dataset","ls","-r","dozer"]
        },
        {
          "name": "dataset create dozer/created",
          "type": "go",
          "request": "launch",
          "mode": "debug",
          "program": "${workspaceFolder}",
          "args": ["--key-file=${workspaceFolder}/key.txt","dataset","create","dozer/created"]
        },
        {
          "name": "dataset create --exec=off dozer/created",
          "type": "go",
          "request": "launch",
          "mode": "debug",
          "program": "${workspaceFolder}",
          "args": ["--key-file=${workspaceFolder}/key.txt","dataset","create","--exec=off","dozer/created"]
        },
        {
          "name": "dataset update --exec=off dozer/created",
