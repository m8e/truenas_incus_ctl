          "args": ["--key-file=${workspaceFolder}/key.txt","snapshot","create","dozer/created@snap"]
        },
        {
          // snapshot listing needs to be more flexible, ie like `zfs list -t snapshot -r dozer/created`
          "name": "snapshot ls -r dozer/created@snap",
          "type": "go",
          "request": "launch",
          "mode": "debug",
          "program": "${workspaceFolder}",
          "args": ["--key-file=${workspaceFolder}/key.txt","snapshot","ls","-r","dozer/created@snap"]
